{
  "name": "buybot-marketplace",
  "version": "1.0.0",
  "description": "AI-powered marketplace with autonomous negotiator agents",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:dev": "cd backend && npx convex dev",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "ai",
    "marketplace",
    "agents",
    "negotiation",
    "convex",
    "nextjs",
    "typescript"
  ],
  "author": "PathSmith Team",
  "license": "MIT",
  "devDependencies": {
    "turbo": "^1.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
